(xbindkey '(control alt t) "xterm")

(xbindkey '(mod4 e) "pcmanfm")

(xbindkey '(mod4 c) "chromium")

(xbindkey '(mod4 a) "gnome-alsamixer")

(xbindkey '(control alt o) "systemctl reboot")

(xbindkey '(control alt p) "systemctl poweroff")

(xbindkey '(control alt a) "import -quality 100 $HOME/Pictures/$(date +%Y-%m-%d-%H:%M:%S).png")
